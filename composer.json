{
  "name": "trunda/php-cors",
  "description": "PHP implementation of CORS which strictly follows specification by it's OOP model design. It supports Laravel framework applications.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jakub Truneček",
      "email": "jakub.trunecek@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "keywords": [
    "laravel",
    "cors",
    "api",
    "cross-origin"
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/http": "5.1.x|5.2.x"
  },
  "autoload": {
    "psr-4": {
      "Trunda\\PhpCors\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "fabpot/php-cs-fixer": "^1.11"
  }
}
